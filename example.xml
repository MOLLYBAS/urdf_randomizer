<?xml version="1.0"?>
<robot name="tumble">

  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>

  <material name="black">
    <color rgba="0.2 0.2 0.2 1"/>
  </material>

  <link name="torso">
    <visual>
      <geometry>
        <box size="0.2 0.14 0.085"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0.0 0"/>
      <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.2 0.14 0.085"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0.0 0"/>
    </collision>
    <inertial>
      <mass value="0.7"/>
      <inertia ixx="0.0110191" ixy="0.0049000" ixz="0.0020825" iyy="0.0062591" iyz="0.0029749" izz="0.0139066" />
      <origin rpy="0 0 0" xyz="0 0.0 0"/>
    </inertial>
  </link>

  <link name="l_leg">
    <visual>
      <geometry>
        <box size="0.128 0.003 0.008"/>
      </geometry>
      <origin rpy="0 1.5707963 0" xyz="0 0.0 0.064"/>
      <material name="green"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.128 0.003 0.008"/>
      </geometry>
      <origin rpy="0 1.5707963 0" xyz="0 0.0 0.064"/>
      <contact_coefficients mu="0.8" />
    </collision>
    <inertial>
      <mass value="0.004"/>
      <inertia ixx="2.1930666e-05" ixy="3.84e-07" ixz="2.4e-08" iyy="9.7333e-08" iyz="1.024e-06" izz="2.1857333e-05" />
      <origin rpy="0 1.5707963 0" xyz="0 0.0 0.064"/>
    </inertial>
  </link>

  <link name="r_leg">
    <visual>
      <geometry>
        <box size="0.128 0.003 0.008"/>
      </geometry>
      <origin rpy="0 1.5707963 0" xyz="0 0.0 0.064"/>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.128 0.003 0.008"/>
      </geometry>
      <origin rpy="0 1.5707963 0" xyz="0 0.0 0.064"/>
      <contact_coefficients mu="0.8" />
    </collision>
    <inertial>
      <mass value="0.004"/>
      <inertia ixx="2.1930666e-05" ixy="3.84e-07" ixz="2.4e-08" iyy="9.7333e-08" iyz="1.024e-06" izz="2.1857333e-05" />
      <origin rpy="0 1.5707963 0" xyz="0 0.0 0.064"/>
    </inertial>
  </link>

  <joint name="torso_l_leg" type="continuous">
    <parent link="torso"/>
    <child link="l_leg"/>
    <axis xyz="-1 0 0"/>
    <limit effort="1000" velocity="100"/>
    <origin rpy="0 0 0" xyz="-0.1095 0.0 0"/>
  </joint>

  <joint name="torso_r_leg" type="continuous">
    <parent link="torso"/>
    <child link="r_leg"/>
    <axis xyz="1 0 0"/>
   <limit effort="1000" velocity="100"/>
   <origin rpy="0 0 0" xyz="0.1095 0.0 0"/>
  </joint>

</robot>
